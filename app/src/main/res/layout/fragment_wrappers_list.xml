                android:layout_height="wrap_content"
                app:cardBackgroundColor="@color/white"
                app:cardCornerRadius="@dimen/wrapper_card_radius"
                app:cardElevation="@dimen/wrapper_card_elevation"
                app:layout_constraintBottom_toBottomOf="@+id/card_series_1"
                app:layout_constraintEnd_toStartOf="@+id/card_series_3"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toEndOf="@+id/card_series_1"
                app:layout_constraintTop_toTopOf="@+id/card_series_1">

                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

