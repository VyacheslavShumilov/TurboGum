            <androidx.cardview.widget.CardView
                android:id="@+id/card_sport_1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="5dp"
                app:cardBackgroundColor="@color/white"
                app:cardCornerRadius="@dimen/wrapper_card_radius"
                app:cardElevation="@dimen/wrapper_card_elevation"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/card_series_1"
                app:layout_constraintStart_toStartOf="@+id/card_series_1"
                app:layout_constraintTop_toBottomOf="@+id/card_series_5"
