                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                app:cardBackgroundColor="@color/white"
                app:cardCornerRadius="@dimen/wrapper_card_radius"
                app:cardElevation="@dimen/wrapper_card_elevation"
                app:layout_constraintBottom_toTopOf="@+id/card_series_5"
                app:layout_constraintEnd_toStartOf="@+id/card_series_2"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.5">

                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <androidx.appcompat.widget.AppCompatImageButton
                        android:id="@+id/image_btn_series_1"
                        android:layout_width="@dimen/btn_img_width"
                        android:layout_height="@dimen/btn_img_height"
                        android:adjustViewBounds="true"
                        android:background="@color/transparent"
                        android:scaleType="fitCenter"
                        android:src="@drawable/t1" />

                    <TextView
                        android:id="@+id/index_series_1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:paddingTop="@dimen/wrapper_card_txt_paddingTop"
                        android:paddingBottom="@dimen/wrapper_card_txt_paddingBottom"
                        android:text="@string/t1_1_50"
                        android:textColor="@color/black"
                        android:textStyle="bold" />

                </androidx.appcompat.widget.LinearLayoutCompat>

            </androidx.cardview.widget.CardView>


            <androidx.cardview.widget.CardView
